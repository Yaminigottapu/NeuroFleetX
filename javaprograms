// finding largest element in an array?
class Main {
    public static void main(String[] args) {
       int arr[]={12,35,1,10,34,1};
       int ans=0;
       for(int i=0;i<arr.length;i++){
           if(ans<arr[i]){
               ans=arr[i];
           }
       }
        System.out.println(ans);
    }
}
