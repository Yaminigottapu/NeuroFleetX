// searching an element in an array?
 class Javaarray2{
    public static void main(String[] args) {
       int arr[]={12,35,1,10,34,1};
       int target=34;
       int ans=0;
       for(int i=0;i<arr.length;i++){
           if(target==arr[i]){
            System.out.println(i);
           }
       }
        
    }
}
